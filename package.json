    "svelte": "^5.43.8",
    "svelte-check": "^4.3.5",
    "svelte-eslint-parser": "^1.4.1",
    "typescript": "^5.9.3",
    "typescript-6": "npm:typescript@^6.0.0-dev.20251227",
    "typescript-svelte-plugin": "^0.3.50",
    "vite": "^7.2.4"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.9.1",
    "@tauri-apps/plugin-dialog": "^2.4.2",
