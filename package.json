    "ts5": "npm run type-check",
    "ts6": "npx typescript@6.0.0-dev.20251227 --noEmit",
    "lint": "eslint src --ext .ts,.js,.svelte",
    "lint:fix": "eslint src --ext .ts,.js,.svelte --fix",
    "format": "prettier --write src/**/*.{ts,js,svelte,json,css}",
    "format:check": "prettier --check src/**/*.{ts,js,svelte,json,css}",
    "quality": "npm run lint && npm run format:check && npm run type-check",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "zip-extensions": "node scripts/zip-extension.cjs"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@sveltejs/adapter-static": "^3.0.10",
